generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id            String   @id @map("_id")
  name          String
  email         String   @unique
  displayName   String
  photoURL      String
  emailVerified Boolean  @default(true)
  createdAt     DateTime @default(now())
  updatedAt     DateTime @default(now())

  @@map("users")
}
